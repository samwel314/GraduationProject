                                                </ul>
                                            </div>

                                        </li>
                                        <li class="nav-item offset-3">
                                            <a class="nav-link text-white    ">   <img src="/Images/WhatsApp Image 2024-01-15 at 22.35.16_597eb9b1.jpg" width="30" height="30" class="rounded-circle border border-white border-2  " />Hello</a>
                                        </li>

                                        <li class="nav-item">
                                            <a class="nav-link text-white   ">Logout </a>
                                        </li>

                                    </ul>


                                </div>
                            </div>
                        </nav>
                    </div>
                    <div class="card-body  body-custom  ">
                        <div class="  create_room create_roombody ">
                            <div class=" card-header bg-primary text-white text-center  create_title mt-3  rounded-top-5">
                                <h4>Manage Trip</h4>
                            </div>
                            <div>
                                <table class="table table-striped table-hover table-light   ">
                                    <thead>
                                        <tr class="small">
                                            <th scope="col">From</th>
                                            <th scope="col">To</th>
                                            <th scope="col">Price</th>
                                            <th scope="col">Start </th>
                                            <th scope="col">End </th>
                                            <th scope="col">Booking Deadline</th>
                                            <th scope="col"> <i class="bi bi-arrow-clockwise"></i> Reset</th>
                                            <th scope="col" class="text-center">Action</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td>luxor</td>
                                            <td>aswan</td>
                                            <td>$7,200.00</td>
                                            <td>Thursday, February 15, 2024</td>
                                            <td>5:32:00 AM</td>
                                            <td>1/30/2024</td>
                                            <td>
                                                <button type="button" class=" btn btn-outline-success btn-sm ">
                                                    <i class="bi bi-arrow-clockwise"></i> Reset
                                                </button>


                                            </td>
                                            <td class="text-center border border-2 border-danger">
                                                <button type="button" class="btn btn-outline-info btn-sm">
                                                    <i class="bi bi-pencil"></i> Update
                                                </button>

                                                <button type="button" class="btn btn-outline-danger btn-sm">
                                                    <i class="bi bi-trash"></i> Delete
                                                </button>
                                            </td>
                                        </tr>
                                        @if (Model.Id != null)
                                        {
                                            <tr>
                                                <td>@Model.From</td>
                                                <td>@Model.To</td>
                                                <td>@Model.Price</td>
                                                <td>@Model.StartAt!.Value.ToLongDateString()</td>
                                                <td>@Model.Duration</td>
                                                <td>1/30/2024</td>
                                                <td>
                                                    <button type="button" class=" btn btn-outline-success btn-sm ">
                                                        <i class="bi bi-arrow-clockwise"></i> Reset
                                                    </button>


                                                </td>
                                                <td class="text-center border border-2 border-danger">
                                                    <button type="button" class="btn btn-outline-info btn-sm">
                                                        <i class="bi bi-pencil"></i> Update
                                                    </button>

                                                    <button type="button" class="btn btn-outline-danger btn-sm">
                                                        <i class="bi bi-trash"></i> Delete
                                                    </button>
                                                </td>
                                            </tr>
                                        }


                                    </tbody>
                                </table>

                            </div>
                        </div>

                    </div>
                </div>
